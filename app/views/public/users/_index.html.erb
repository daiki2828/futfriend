<table class='table table-borderless table-hover mt-3'>
  <% users.each do |user| %>
  <tbody>
    <tr>
      <td><%= image_tag user.get_profile_image, style: "border-radius: 50%; width: 50px; height: 50px;" %></td>
      <td><%= user.name %></td>
      <td><%= user.prefacture_name %></td>
      <td><%= render "public/relationships/btn", user: user %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
    </tr>
  </tbody>
  <% end %>
</table>